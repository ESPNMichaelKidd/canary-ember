
<div class="navbar navbar-default" role="navigation">
  <div class="container-fluid">
    <div class="navbar-header">
      <span class="navbar-brand">Overview</span>
    </div>
    <div class="navbar-collapse collapse">
      <ul class="nav navbar-nav">
        <li class="dropdown">
        <a href="#" class="dropdown-toggle" data-toggle="dropdown">Location : {{controllers.measurements.currentLocation}}<b class="caret"></b></a>
          <ul class="dropdown-menu">
            <li>{{#link-to 'measurements' (query-params currentLocation="all")}}All Locations{{/link-to}}</li>
            <li class="divider"></li>
            {{#each location in uniqLocations}}
              <li>
              {{#link-to 'measurements' (query-params currentLocation=location)}}{{location}}{{/link-to}}
              </li>
            {{/each}}
          </ul>
        </li>
      </ul>
      <ul class="nav navbar-nav navbar-right">
      </ul>
    </div><!--/.nav-collapse -->
  </div><!--/.container-fluid -->
</div>





<div class="overview clearfix">
  <div class="timing">
    <h3>{{#link-to 'measurements.timing'}}Timing{{/link-to}}</h3>
    {{nvd3-chart data=currentLineData showLegend=true}}
  </div>
  <div class="http-response">
    <h3>{{#link-to 'measurements.response_codes'}}Http Status{{/link-to}}</h3>
    {{nvd3-donut-chart data=currentHttpPieData}}
  </div>
</div>

<hr/>

<h3>Measurments per 3 min </h3>
{{nvd3-chart dataBinding=currentRunningTotalsData showLegend=true height="130px"}}

<hr/>
<div class="pies">
  <div class="pie ">
    <h3>Primary IP</h3>
    {{nvd3-donut-chart data=currentPrimaryIpPieData}}
  </div>
  <div class="pie">
    <h3>Local IP</h3>
    {{nvd3-donut-chart data=currentLocalIpPieData}}
  </div>
  <div class="pie">
    <h3>Exit Status</h3>
    {{nvd3-donut-chart data=currentExitPieData}}
  </div>
  {{#if isOverview }}
  <div class="pie ">
    <h3>Location</h3>
    {{nvd3-donut-chart data=currentLocationPieData}}
  </div>
  {{/if}}
</div>
